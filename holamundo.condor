universe 				= docker
docker_image			= frolvlad/alpine-gcc
executable = gcc
transfer_executable 	= false
arguments				= --static holamundo.c -o holamundo
should_transfer_files   = YES
transfer_input_files    = holamundo.c
when_to_transfer_output = ON_EXIT
output                  = out.$(Cluster)
error                   = err.$(Cluster)
log                     = log.$(Cluster)
queue 1
